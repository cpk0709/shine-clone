{
  "name": "obliv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ts": "node --loader ts-node/esm",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next dev --port 8888",
    "lint": "next lint",
    "lint:fix": "eslint --fix **/*.tsx **/*.ts",
    "serve": "next start --port 3000",
    "webp": "npm pkg set 'type'='module' && yarn ts ./src/webp.ts && npm pkg delete 'type'",
    "webp:mv": "npm pkg set 'type'='module' && yarn ts ./src/webp.ts && npm pkg delete 'type' && mv ./public/obliv/new/* ./public/obliv/"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
