    "@tanstack/react-query": "^5.7.2",
    "@tanstack/react-query-devtools": "^5.7.2",
    "@tinymce/tinymce-react": "^4.3.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "next": "13.5.6",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-lazy-load-image-component": "^1.6.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "swiper": "^11.0.3"
  },
  "devDependencies": {
